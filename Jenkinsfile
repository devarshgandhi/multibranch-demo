node {
   // Mark the code checkout 'stage'....
   stage 'Checkout'

   // Checkout code from repository
   checkout scm
   
   // Mark the code build 'stage'....
   stage 'Build'
   // Print Statement
   echo "This is Test Build"
   
   stage 'Send Email'
   //print Statement
   echo "Email with test results and artifacts is sent to the user group"
}
